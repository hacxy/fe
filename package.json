{
  "name": "@hacxy/create-fe",
  "type": "module",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.12.3",
  "description": "",
  "author": {
    "name": "Hacxy",
    "email": "hacxy.js@outlook.com",
    "url": "https://hacxy.cn"
  },
  "license": "MIT",
  "keywords": [],
  "bin": {
    "create-fe": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "npm run copy-templates && tsdown",
    "build": "npm run copy-templates && tsdown --minify",
    "watch": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:all": "pnpm -r run lint",
