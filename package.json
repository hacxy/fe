{
  "name": "fe",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.12.3",
  "description": "",
  "author": {
    "name": "Hacxy",
    "email": "hacxy.js@outlook.com",
    "url": "https://hacxy.cn"
  },
  "license": "MIT",
  "keywords": [
    "cli",
    "fe",
    "create-fe"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "build:cli": "npm run copy-templates && pnpm -F @hacxy/fe build && pnpm -F @hacxy/create-fe build",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "lint:staged": "pnpm -r run lint:staged",
    "cz": "czg",
    "copy-templates": "node scripts/copy-template.mjs",
    "release": "npm run copy-templates && pnpm -F @hacxy/fe bump && pnpm -F @hacxy/create-fe bump",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@hacxy/bump": "^0.2.0",
    "@types/node": "^22.16.5",
    "chalk": "^5.4.1",
    "czg": "^1.12.0",
    "inquirer": "^12.9.0",
    "lint-staged": "^16.1.2",
    "ora": "^8.2.0",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "node scripts/commit-msg.mjs"
  },
  "lint-staged": {
    "*": [
      "npm run lint:staged"
    ]
  }
}
