    "release": "bump",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@hacxy/bump": "^0.1.0",
    "@hacxy/eslint-config": "^0.4.1",
    "@hacxy/tsconfig": "^0.1.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.16.5",
    "chalk": "^5.4.1",
    "czg": "^1.12.0",
    "eslint": "^9.32.0",
    "inquirer": "^9.3.7",
    "lint-staged": "^16.1.2",
    "ora": "^8.2.0",
    "simple-git-hooks": "^2.13.0",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "node scripts/commit-msg.mjs"
  },
  "lint-staged": {
    "*": [
      "npm run lint:staged"
    ]
  }
